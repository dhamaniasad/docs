{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "MemoryPlugin",
  "colors": {
    "primary": "#6366F1",
    "light": "#8B5CF6",
    "dark": "#4338CA"
  },
  "favicon": "/favicon.svg",
  "openapi": "/openapi.json",
  "metadata": {
    "og:title": "MemoryPlugin Documentation",
    "og:description": "Cure your AI of amnesia with long-term memory across all your favorite AI platforms",
    "og:image": "/images/opengraph-image.png",
    "twitter:title": "MemoryPlugin Documentation",
    "twitter:description": "Cure your AI of amnesia with long-term memory across all your favorite AI platforms",
    "twitter:image": "/images/twitter-image.png",
    "twitter:card": "summary_large_image"
  },
  "analytics": {
    "gtm": {
      "tagId": "GTM-XXXXXXX"
    }
  },
  "search": {
    "prompt": "Search MemoryPlugin docs..."
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "getting-started/introduction",
              "getting-started/how-it-works"
            ]
          },
          {
            "group": "Integrations",
            "pages": [
              "integrations/mcp-server",
              "integrations/browser-extension",
              "integrations/custom-gpt",
              "integrations/custom-gpt-integration",
              "integrations/typingmind-plugin",
              "integrations/api-integration"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "features/memory-buckets",
              "features/smart-memory",
              "features/dashboard",
              "features/chat-history-import",
              "features/bulk-operations"
            ]
          },
          {
            "group": "Platforms",
            "pages": [
              "platforms/supported-platforms",
              "platforms/chatgpt",
              "platforms/claude",
              "platforms/gemini"
            ]
          },
          {
            "group": "Teams",
            "pages": [
              "teams/typingmind-custom"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Memory Endpoints",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/search-memories"
            ]
          },
          {
            "group": "Bucket Endpoints",
            "pages": [
              "api-reference/endpoint/get-buckets",
              "api-reference/endpoint/create-bucket"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://memoryplugin.com/dashboard",
          "icon": "gauge"
        },
        {
          "anchor": "Website",
          "href": "https://memoryplugin.com",
          "icon": "globe"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@memoryplugin.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Pricing",
        "href": "https://memoryplugin.com/#pricing"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://memoryplugin.com/dashboard"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/memoryplugin",
      "github": "https://github.com/memoryplugin"
    }
  }
}
